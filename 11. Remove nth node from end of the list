Date : 10/02/2026
class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {

            if(head == null || head.next == null)
            {
                return null;
            }
              ListNode dummy = new ListNode(0);

              dummy.next=head;

              ListNode slow=dummy , fast=dummy;

              for(int i=1;i<=n;i++)
              {
                fast=fast.next;
              }

              while(fast.next!=null)
              {
                slow = slow.next;
                fast=fast.next;
              }
              slow.next=slow.next.next;
              return dummy.next;
    }
}
