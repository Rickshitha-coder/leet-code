Date : 07/02/2026
class Solution {
    public ListNode swapNodes(ListNode head, int k) {
        ListNode temp = head , second = head;

        for(int i=1;i<k;i++)
        {
            temp=temp.next;
            
        }
        ListNode first=temp;
        while(temp.next!=null)
        {
            temp=temp.next;
            second=second.next;
        }
        int t=first.val;
        first.val=second.val;
        second.val=t;
        return head;
    }
}
