Date : 20/02/2026

class Solution {
    public int evalRPN(String[] tokens) {
        Stack<Integer> st=new Stack<Integer>();

        for(String str: tokens)
        {
            if(str.equals("+") || str.equals("-") || str.equals("*") || str.equals("/"))
            {
                int b=st.pop();
                int a=st.pop();

                int res=0;
                if(str.equals("+"))
                {
                    res=a+b;
                }
                else if(str.equals("-"))
                {
                    res=a-b;
                }
                else if(str.equals("*"))
                {
                    res=a*b;
                }
                else if(str.equals("/"))
                {
                    res=a/b;
                }
                st.push(res);
            }
            else
            {
                st.push(Integer.parseInt(str));
            }
        }
        return st.pop();
        }
    }
